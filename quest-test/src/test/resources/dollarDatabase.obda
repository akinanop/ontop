[PrefixDeclaration]
:		http://www.semanticweb.org/small$Database#

[SourceDeclaration]
sourceUri	SmallDataset
connectionUrl	jdbc:h2:mem:dollar
username	sa
password	
driverClass	org.h2.Driver

[MappingDeclaration] @collection [[
mappingId	mappingAddress
target		<http://www.semanticweb.org/small$Database#address$-{id}> a :Address ; :inStreet {str$eet} ; <http://www.semanticweb.org/small$Database#hasNumber$id> {number$} ; :inCity {city} .
source		select id, str$eet, number$, city from address$


]]

